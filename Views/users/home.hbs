  <section id="mc-section-4" class="mc-section-3 section">
    <div class="container">
      <div class="module so-deals">
        <h3 class="modtitle">
          <span>Lộ trình học tập</span>
        </h3>
        <a href="/coursesRoute" title="Tất cả các lộ trình học tập" class="all-course mc-btn btn-style-1">Xem tất cả</a>
        <div class="module-body">
          <div class="owl-carousel owl-theme" style="display: block; opacity: 1;">
            <!-- <div class="owl-wrapper-outer">
            <div class="owl-wrapper" style="width: 5508px;left: 0px;display: block;"> -->
            <!-- angular -->
            {{#each route}}
            <div class="item" style="width: auto;">
              <div class="learning-path mc-item mc-item-1">
                <div class="image-heading">
                  <a href="#" title="{{name}}"><img style="max-height: 180px;" title="{{name}}"
                      src="../../img/{{image}}" alt="Lo trinh hoc angular"></a>

                </div>
                <div class="content-item">
                  <h4 href="#" title="{{name}}">{{name}}</h4>
                </div>
              </div>
            </div>
            {{/each}}
          </div>
        </div>
      </div>
    </div>
  </section>
{{!-- khoa hoc tieu bieu --}}
  <section id="mc-section-4" class="mc-section-3 section">
    <div class="container">
      <div class="module so-deals">
        <h3 class="modtitle">
          <span>Khoa học tiêu biểu</span>
        </h3>
        <a href="/courses" title="Tất cả các lộ trình học tập" class="all-course mc-btn btn-style-1">Xem tất cả</a>
        <div class="module-body">
          <div class="owl-carousel owl-theme" style="display: block; opacity: 1;">
            {{#each courses}}
            <div class="item" style="width: auto;">
              <div class="mc-item mc-item-1">
                <div class="image-heading">
                  <a href="#" title="{{name}}"><img style="max-height: 180px;" title="{{name}}"
                      src="../../img/{{image}}" alt="Xay dung khoa hoc"></a>
                </div>
                <div class="meta-categories">
                  <a href="#" title="{{name}}">{{name}}</a>
                </div>
                <div class="content-item">
                  <div class="image-author">
                    <a href="#" title="Giảng viên: Phạm Đình Long">...</a>
                  </div>
                  <h4><a href="#" title="{{name}}">{{name}}</a></h4>
                  <div class="name-author">
                    <span>Bởi
                      <a href="#" title="Giảng viên: Phạm Đình Long">Phạm Đình Long</a>
                    </span>
                  </div>
                </div>
                <div class="ft-item">
                  <div class="rating">
                    <span class="fa fa-star checked"></span>
                    <span class="fa fa-star checked"></span>
                    <span class="fa fa-star checked"></span>
                    <span class="fa fa-star"></span>
                    <span class="fa fa-star"></span>
                  </div>
                  <div class="view-info">
                    <i class="fa fa-users" aria-hidden="true"></i>16
                  </div>
                  <div class="price">
                    <span>{{price}} $</span>
                  </div>
                </div>
              </div>
            </div>
            {{/each}}
          </div>
        </div>
      </div>
    </div>
  </section>
